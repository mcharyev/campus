{% extends 'main.html.twig' %}
{% block customcss %}
    <style>
        .extrasmall {font-size:10px;}
        .main {background-color:#ebebeb;}
        .columnName {vertical-align:middle;}
        .low { color: #d6d6d4;}
        .normal {color: black;}
        .red {color: red; font-weight:bold;}
        th { background-color: #F5F5F5}
        tfoot {font-weight:bold; font-size:12px; background-color: #F9F9F9}
        .centered {
            vertical-align:middle;text-align:center;
        }

    </style>
{% endblock %}
{% block moduletitle %}Work Report{% endblock %}
{% block main %}
    <div class="no-print" style="margin:auto;width:auto;">
        <div style="margin:auto;">
            <h5>Teacher Workset Report</h5>
            <h6>Name: {{ teacher.fullname }}</h6> 
            <h6>Department: 
                {% if is_granted("ROLE_SPECIALIST") %}
                    <a href='/faculty/departmentworkitem/department/{{ teacherWorkSet.department.systemid }}/{{ year }}'>{{ teacherWorkSet.department.nameEnglish }}</a>
                {% else %}
                    {{ teacherWorkSet.department.nameEnglish }}
                {% endif %}
            </h6>
            <h6>
                <span class='bold'>Workset Plans:</span> 
                {% for teacherWorkSet in teacher.teacherWorkSets %}
                    <a href="/faculty/teacherworkset/view/{{ teacher.id }}/{{ teacherWorkSet.id }}/{{ year }}">{{ teacherWorkSet.shortTitle }}</a> |
                {% endfor %}
                {% if is_granted("ROLE_SPECIALIST") %}
                    <a href="/faculty/teacherworkset/editinteacher/0/{{ teacher.id }}/{{ year }}/0/{{ teacher.department.id }}">Add new</a>
                {% endif %}
            </h6>
            <h6>
                Workset Reports: 
                {% for teacherWorkSet in teacher.teacherWorkSets %}
                    <a href='/faculty/teacherworkreport/{{ teacher.id }}/{{ teacherWorkSet.id }}/{{ year }}/{{ semester }}'>{{ teacherWorkSet.shortTitle }}</a> |
                {% endfor %}
                {% if is_granted("ROLE_SPECIALIST") %}
                    <a href='/faculty/teacherworkset/edit/{{ teacherWorkSet.id }}'>Edit</a>
                {% endif %}
            </h6>
            <h6>
                <span class='bold'>Workset Journals:</span> 
                {% for teacherWorkSet in teacher.teacherWorkSets %}
                    <a href="/faculty/teacherjournalnew/{{ teacher.id }}/{{ teacherWorkSet.id }}/{{ year }}/{{ semester }}">{{ teacherWorkSet.shortTitle }}</a> |
                {% endfor %}
            </h6>

        </div>
    </div>
    <div>
        <center>
            <div style="display:none;">
                Department: <span class='excellable' data-excel='{"cell":"C4"}'>{{ teacher.department.nameTurkmen }}</span>
                <br>
                <h4>Workset Report for: 
                    <span class='excellable' data-excel='{"cell":"C3"}'>{{ teacher.fullname }}</span>
                    (<span class='excellable' data-excel='{"cell":"C5"}'>{{ workloadNames[teacherWorkSet.workload] }}</span>)
                </h4>
            </div>
            {% set monthNamesEnglish = ['January', 'February', 'March', 'April', 'May', 'June', 'July', 'August', 'September', 'October', 'November', 'December'] %}
            {% set monthNamesTurkmen = ['ýanwar', 'fewral', 'mart', 'aprel', 'maý', 'iýun', 'iýul', 'awgust', 'sentýabr', 'oktýabr', 'noýabr', 'dekabr'] %}
            {% set monthNamesTurkmen = ['ýanwar', 'fewral', 'mart', 'aprel', 'maý', 'iýun', 'iýul', 'awgust', 'sentýabr', 'oktýabr', 'noýabr', 'dekabr'] %}
            {% set semesterNames = ['','I', 'II', 'III'] %}
            {% for month in months %}
                <div>
                    Month: {{ monthNamesTurkmen[month - 1] }}&nbsp;&nbsp;&nbsp;
                    <button onclick="download('template_teacher_monthly_report.xlsx', 'teacher_monthly_report_{{ teacher.fullname }}_{{ month }}.xls', 10,{{ month }} , '{{ monthNamesTurkmen[month - 1] }}', '{{ semesterNames[semester] }}',{{ year }} )">xlsx</button></div>
                <table class='table table-sm table-bordered extrasmall'>
                    <thead>
                        <tr>
                            {% for workColumn in workColumns %}
                                <th> {{ workColumn }}</th>
                                {% endfor %}
                        </tr>
                        <tr>
                            {% for workColumn in workColumns %}
                                <th class='centered'> {{ workColumnTexts[workColumn] }}</th>
                                {% endfor %}
                        </tr>
                    </thead>
                    <tbody>
                        {% for workitem in workitems[month] %}
                            <tr class='excellablerow_{{ month }}'>
                                <td class='centered'>{{ workitem.number }}</td>
                                <td class='centered'>{{ workitem.title }}</td>
                                <td class='centered'>{{ workitem.groups }}</td>
                                {% for u in 4..28 %}
                                    {% if workitem.courseData[u] == 0  %}
                                        {% set colorClass = 'low' %}
                                    {% else %}
                                        {% set colorClass = 'normal' %}
                                    {% endif %}
                                    <td class='{{ colorClass }} centered'>{{ workitem.courseData[u] }}</td>
                                {% endfor %}
                                {% if workitem.courseData[29] == 0  %}
                                    {% set colorClass = 'low' %}
                                {% else %}
                                    {% set colorClass = 'normal' %}
                                {% endif %}
                                <td class='{{ colorClass }} centered'>{{ workitem.courseData[29] }}</td>
                            </tr>
                            {% if workitem.replacedSessionsSums[29]>0 %}
                                <tr class='excellablerow_{{ month }}'>
                                    <td class='centered'></td>
                                    <td class='centered'>Substitutions</td>
                                    <td class='centered'></td>
                                    {% for u in 4..28 %}
                                        <td class='centered'>{{ workitem.replacedSessionsSums[u] }}</td>
                                    {% endfor %}
                                    <td class='centered'>{{ workitem.replacedSessionsSums[29] }}</td>
                                </tr>
                            {% endif %}
                        {% endfor %}
                    </tbody>
                    <tfoot>
                        <tr id='footerrow_{{ month }}' class='boldtext'>
                            <td></td>
                            <td style='vertical-align:middle;text-align:center;'>Jemi</td>
                            <td></td><td></td><td></td><td></td><td></td><td></td>
                            {% for u in 9..29 %}
                                {% if monthSums[month][u] == 0  %}
                                    {% set colorClass = 'low' %}
                                {% else %}
                                    {% set colorClass = 'normal' %}
                                {% endif %}
                                <td class='{{ colorClass }}' style='vertical-align:middle;text-align:center;'>
                                    {{ monthSums[month][u] }}
                                </td>
                            {% endfor %}
                        </tr>
                    </tfoot>
                </table>
            {% endfor %}
            <div>Summary by Type 
                <button onclick="downloadSemester('template_teacher_semester_report.xlsx', 'teacher_semester_report_{{ teacher.fullname }}_{{ semester }}.xls', 10, {{ semester }}, '{{ semesterNames[semester] }}',{{ year }} )">xlsx</button></div>
            <table class='table table-sm table-bordered extrasmall'>
                <thead>
                    <tr>
                        {% for workColumn in workColumns %}
                            <th> {{ workColumn }}</th>
                            {% endfor %}
                    </tr>
                    <tr>
                        {% for workColumn in workColumns %}
                            <th style='vertical-align:middle;text-align:center;'> {{ workColumnTexts[workColumn] }}</th>
                            {% endfor %}
                    </tr>
                </thead>
                <tbody>
                    {% set colorClass = 'normal' %}
                    {% for workitem in workitemsData %}
                        <tr class='semester_excellablerow'>
                            <td style='vertical-align:middle;text-align:center;'>{{ workitem.number }}</td>
                            <td style='vertical-align:middle;text-align:center;'>{{ workitem.title }}</td>
                            <td style='vertical-align:middle;text-align:center;'>{{ workitem.groups }}</td>
                            {% for u in 4..28 %}
                                {% if workitem.item.data[u] == 0 %}
                                    {% set colorClass = 'low' %}
                                {% else %}
                                    {% if u > 9 %}
                                        {% if workitem.item.data[u] != workitem.item_sums[u]  %}
                                            {% if u!=9 and u!=11 and u!=13  %}
                                                {% set colorClass = 'red' %}
                                            {% endif %}
                                        {% else %}
                                            {% set colorClass = 'normal' %}
                                        {% endif %}
                                    {% endif %}
                                {% endif %}
                                {% if u > 8 %}
                                    {% if ((u != 9 and u != 11 and u != 13)) %}
                                        <td class='{{ colorClass }}' style='vertical-align:middle;text-align:center;'>
                                            {{ workitem.item_sums[u] }} / {{ workitem.item.data[u] }}
                                        </td>
                                    {% else %}
                                        <td class='{{ colorClass }}' style='vertical-align:middle;text-align:center;'>0</td>
                                    {% endif %}
                                {% else %}
                                    <td class='{{ colorClass }}' style='vertical-align:middle;text-align:center;'>
                                        {{ workitem.item.data[u] }}
                                    </td>
                                {% endif %}
                            {% endfor %}
                            {% if workitem.item.data[29] == 0  %}
                                {% set colorClass = 'red' %}
                            {% else %}
                                {% set colorClass = 'normal' %}
                            {% endif %}
                            <td class='{{ colorClass }}' style='vertical-align:middle;text-align:center;'>
                                {{ workitem.item_sums[29] }} / {{ workitem.item.data[29] }}
                            </td>
                        </tr>
                        <tr class='semester_excellablerow'>
                            <td class='centered'></td>
                            <td class='centered'>Substitutions</td>
                            <td class='centered'></td>
                            {% for u in 4..28 %}
                                <td class='centered'>
                                    {{ workitem.replacedSessionsTotalSums[u] }}
                                </td>
                            {% endfor %}
                            <td class='centered'>
                                {{ workitem.replacedSessionsTotalSums[29] }}
                            </td>
                        </tr>
                    {% endfor %}
                </tbody>
                <tfoot>
                    <tr id='semester_footerrow'>
                        <td></td>
                        <td style='vertical-align:middle;text-align:center;'>TOTAL</td>
                        <td></td><td></td><td></td><td></td><td></td><td></td>
                        {% for u in 9..29 %}
                            {% if totalTypeSums[u] == 0 and totalLoadSums[u] == 0  %}
                                {% set colorClass = 'low' %}
                            {% else %}
                                {% if u >= 9 %}
                                    {% if totalTypeSums[u] != totalLoadSums[u]  %}
                                        {% if u!=9 and u!=11 and u!=13  %}
                                            {% set colorClass = 'red' %}
                                        {% endif %}
                                    {% else %}
                                        {% set colorClass = 'normal' %}
                                    {% endif %}
                                {% endif %}
                            {% endif %}
                            {% if ((u != 9 and u != 11 and u != 13)) %}
                                <td class='{{ colorClass }}' style='vertical-align:middle;text-align:center;'>
                                    {{ totalTypeSums[u] }} / {{ totalLoadSums[u]  }}
                                </td>
                            {% else %}
                                <td class='{{ colorClass }}' style='vertical-align:middle;text-align:center;'>
                                    0
                                </td>
                            {% endif %}
                        {% endfor %}
                    </tr>
                </tfoot>
            </table><br>

            <div>
                Summary by Month
                <button onclick="downloadMonthly('template_teacher_semester_report.xlsx', 'teacher_semester_monthly_report_{{ teacher.fullname }}_{{ semester }}.xls', 10, {{ semester }}, '{{ semesterNames[semester] }}',{{ year }} )">xlsx</button>
            </div>
            <table class='table table-sm table-bordered extrasmall'>
                <thead>
                    <tr>
                        {% for workColumn in workColumns %}
                            <th> {{ workColumn }}</th>
                            {% endfor %}
                    </tr>
                    <tr>
                        {% for workColumn in workColumns %}
                            <th style='vertical-align:middle;text-align:center;'> {{ workColumnTexts[workColumn] }}</th>
                            {% endfor %}
                    </tr>
                </thead>
                <tbody>
                    {% for month in months %}
                        <tr class='semester_excellablerow_monthly'>
                            <td style='vertical-align:middle;text-align:center;'>{{ loop.index }}</td>
                            <td style='vertical-align:middle;text-align:center;'>{{ monthNamesEnglish[month-1] }}</td>
                            <td></td><td></td><td></td><td></td><td></td><td></td>
                            {% for u in 9..29 %}
                                {% if monthSums[month][u] == 0  %}
                                    {% set colorClass = 'low' %}
                                {% else %}
                                    {% set colorClass = 'normal' %}
                                {% endif %}
                                <td class='{{ colorClass }}' style='vertical-align:middle;text-align:center;'>
                                    {{ monthSums[month][u] }}
                                </td>
                            {% endfor %}
                        </tr>
                    {% endfor %}
                </tbody>
                <tfoot>
                    <tr id='semester_footerrow_monthly'>
                        <td></td>
                        <td style='vertical-align:middle;text-align:center;'>TOTAL</td>
                        <td></td><td></td><td></td><td></td><td></td><td></td>
                        {% for u in 9..29 %}
                            {% if totalTypeSums[u] == 0  %}
                                {% set colorClass = 'low' %}
                            {% else %}
                                {% set colorClass = 'normal' %}
                            {% endif %}
                            <td class='{{ colorClass }}' style='vertical-align:middle;text-align:center;'>
                                {{ totalTypeSums[u] }}
                            </td>
                        {% endfor %}
                    </tr>
                </tfoot>
            </table><br>
            <div>
                Substitutions Received
                <table class="table table-sm table-bordered extrasmall" style="width:auto;">
                    <thead>
                    <th>No.</th>
                    <th>Course name</th>
                    <th>Substituted for</th>
                    <th>Substituted by</th>
                    <th>Old date</th>
                    <th>New date</th>
                    <th>Old session</th>
                    <th>New session</th>
                    <th>Type</th>
                    <th>Hours</th>
                    </thead>
                    <tbody>
                        {% set substituteSum = 0 %}
                        {% for substitution in substitutionsReceived %}
                            {% if substitution.newTeacher.id != teacher.id %}
                                <tr class='substitutions_excellablerow'>
                                    <td>{{ loop.index }}</td>
                                    <td>{{ substitution.scheduleItem.taughtCourse.nameEnglish  }}</td>
                                    <td>
                                        {{ substitution.scheduleItem.taughtCourse.teacher.shortFullname }}
                                    </td>
                                    <td>
                                        {{ substitution.newTeacher.shortFullname }}
                                    </td>
                                    <td>
                                        {{ substitution.date|date('d.m.Y') }}
                                    </td>
                                    <td>
                                        {{ substitution.newDate|date('d.m.Y') }}
                                    </td>
                                    <td>
                                        {{ substitution.session }}  
                                    </td>
                                    <td>
                                        {{ substitution.newSession }}  
                                    </td>
                                    <td>
                                        {{ substitution.classType.nameEnglish }}  
                                    </td>
                                    <td>
                                        {{ substitution.classType.hours }}
                                        {% set substituteSum = substituteSum + substitution.classType.hours %}
                                        {% if is_granted("ROLE_SPECIALIST") %}
                                            &nbsp;&nbsp;&nbsp;<a href='javascript:deleteScheduleChange({{ substitution.id }});'>Delete</a>
                                        {% endif %}
                                        <span id='sinfo{{ substitution.id }}'></span>
                                    </td>
                                </tr> 
                            {% endif %}
                        {% endfor %}
                    </tbody>
                    <tfoot>
                        <tr id='substitutions_footerrow'>
                            <td>Total hours</td>
                            <td></td>
                            <td></td>
                            <td></td>
                            <td></td>
                            <td></td>
                            <td></td>
                            <td></td>
                            <td>{{ substituteSum }}</td>
                        </tr>
                    </tfoot>
                </table>
            </div>
            <div>
                Substitutions Delivered
                <table class="table table-sm table-bordered extrasmall" style="width:auto;">
                    <thead>
                    <th>No.</th>
                    <th>Course name</th>
                    <th>Substituted for</th>
                    <th>Substituted by</th>
                    <th>Old date</th>
                    <th>New date</th>
                    <th>Old session</th>
                    <th>New session</th>
                    <th>Type</th>
                    <th>Hours</th>
                    </thead>
                    <tbody>
                        {% set substituteSum = 0 %}
                        {% for substitution in substitutionsReceived %}
                            {% if substitution.newTeacher.id == teacher.id %}
                                <tr class='substitutions_excellablerow'>
                                    <td>{{ loop.index }}</td>
                                    <td>{{ substitution.scheduleItem.taughtCourse.nameEnglish  }}</td>
                                    <td>
                                        {{ substitution.scheduleItem.taughtCourse.teacher.shortFullname }}
                                    </td>
                                    <td>
                                        {{ substitution.newTeacher.shortFullname }}
                                    </td>
                                    <td>
                                        {{ substitution.date|date('d.m.Y') }}
                                    </td>
                                    <td>
                                        {{ substitution.newDate|date('d.m.Y') }}
                                    </td>
                                    <td>

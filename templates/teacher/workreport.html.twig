                <button onclick="downloadSubstitutions('template_teacher_semester_substitutions.xlsx', 'teacher_semester_substitutions_{{ teacher.fullname }}_{{ semester }}.xls', 10, {{ semester }}, '{{ semesterNames[semester] }}',{{ year }} )">xlsx</button></div>
            <table class="table table-sm table-bordered extrasmall" style="width:auto;">
                <thead>
